<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map>
<!-- Sample Mapnik XML -->
<Map srs="+proj=latlong +datum=WGS84 +k=1.0 +units=m +over +no_defs">
  <Style name="cali">
    <Rule>
      <Filter>[STATE] = 'California'</Filter>
      <PolygonSymbolizer fill="#cfdea7" />
    </Rule>
  </Style>
  <Style name="mtn">
    <Rule>
      <PointSymbolizer file="./viewer/images/crosshair16x16.png" />
    </Rule>
  </Style>

</Map>
