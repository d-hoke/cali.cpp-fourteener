<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map>
<!-- Sample Mapnik XML -->
<Map srs="+proj=latlong +datum=WGS84 +k=1.0 +units=m +over +no_defs">
  <Style name="cali">
    <Rule>
      <Filter>[STATE] = 'California'</Filter>
      <PolygonSymbolizer>
	<CssParameter name="fill">#cfdea7</CssParameter>
      </PolygonSymbolizer>
    </Rule>
  </Style>
  <Style name="mtn">
    <Rule>
      <PointSymbolizer file="./viewer/images/crosshair16x16.svg" type="svg" width="16" height="16" allow_overlap="true"/>
    </Rule>
  </Style>

</Map>
